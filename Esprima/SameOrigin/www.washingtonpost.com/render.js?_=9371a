$(function(){var publishPage=$("meta[name\x3d'publishPage']").attr("content");if(publishPage=="false")window.location.replace("http://washingtonpost.com");else{var pod=TWP.Features.Podcast.Player;if(!pod.PoDaLoaded){var playerContainer=new pod.AudioPlayerContainer;var playerUIs=$(".podcast-play-action").toArray();for(var player in playerUIs)playerContainer.addPlayer(playerUIs[player]);pod.PoDaLoaded=true}}});